<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!--  设置mybatis输出日志 标准日志-->
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <!-- 设置实体类别名 -->
    <typeAliases>
        <package name="com.zjl.crm.settings.domain"/>
        <package name="com.zjl.crm.workbench.domain"/>
    </typeAliases>
    <!--  指定sql mapper的位置-->
    <mappers>
        <package name="com.zjl.crm.settings.dao"/>
        <package name="com.zjl.crm.workbench.dao"/>
    </mappers>
</configuration>